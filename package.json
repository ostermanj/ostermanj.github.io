{
	"name": "ostermanj-io",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"dev:expose": "vite dev --host",
		"dev:debug": "node --inspect node_modules/.bin/vite dev",
		"build:local": "cross-env NODE_ENV=production vite build",
		"build": "cross-env NODE_ENV=production vite build",
		"build:debug": "node --inspect node_modules/.bin/vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "^1.15.2",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"contentful": "^9.1.33",
		"cross-env": "^7.0.3",
		"env-cmd": "^10.1.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-sass": "^8.0.0",
		"normalize.css": "^8.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.58.1",
		"slugify": "^1.6.5",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4"
	},
	"type": "module",
	"dependencies": {
		"markdown-it": "^13.0.1",
		"svelte-paginate": "github:ostermanj/svelte-paginate"
	}
}
